metropolis <- function(leng, star, posterior, sd){
values <- c(star,c(1:leng))
for (i in 2:length(values)){
old <- values[i-1]
new <- star + rnorm(1,mean = 0, sd = sd)
acceptan <- acceptance(new, old, posterior)
if (acceptan > runif(1)){
values[i] <- new
}else{
values[i] <- old
}
}
return(values[2:length(values)])
}
